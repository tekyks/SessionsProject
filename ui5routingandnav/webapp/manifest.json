{
    "_version": "1.65.0",
    "sap.app": {
        "id": "ui5routingandnav",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "0.0.1"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "sourceTemplate": {
            "id": "@sap/generator-fiori:basic",
            "version": "1.15.6",
            "toolsId": "8bb70f16-7102-48c4-bf25-78acf51a3ade"
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": false,
        "dependencies": {
            "minUI5Version": "1.141.3",
            "libs": {
                "sap.m": {},
                "sap.ui.core": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "ui5routingandnav.i18n.i18n"
                }
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "controlAggregation": "pages",
                "controlId": "app",
                "transition": "slide",
                "type": "View",
                "viewType": "XML",
                "path": "ui5routingandnav.view",
                "async": true,
                "viewPath": "ui5routingandnav.view"
            },
            "routes": [
                {
                    "name": "route_name_view1",
                    "target": "target_view1",
                    "pattern": ""
                },
                {
                    "name": "route_name_view2",
                    "target": "target_view2",
                    "pattern": "PatternView2"
                }
            ],
            "targets": {
                "target_view1": {
                    "id": "View1",
                    "name": "View1"
                },
                "target_view2": {
                    "id": "View2",
                    "name": "View2"
                }
            }
        },
        "rootView": {
            "viewName": "ui5routingandnav.view.App",
            "type": "XML",
            "id": "App"
        }
    }
}